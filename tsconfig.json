{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2016",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "noEmit": true,
    "lib": ["es2022", "dom", "dom.iterable"],
    "strict": false,
    "noUncheckedIndexedAccess": true
  }
}
